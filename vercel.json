{
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "env": {
    "ENABLE_LAZY_LOADING": "@enable_lazy_loading",
    "ENABLE_VIRTUAL_SCROLL": "@enable_virtual_scroll",
    "ENABLE_PREDICTIVE_LOADING": "@enable_predictive_loading",
    "ENABLE_MEMORY_POOLING": "@enable_memory_pooling",
    "PERFORMANCE_MODE": "@performance_mode"
  }
}